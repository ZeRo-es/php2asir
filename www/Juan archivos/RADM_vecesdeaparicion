<?php


$vector=[];

// ¡Generar una canatidad aleatoria de $temp!
for ($i=1; $i <= 50; $i++) {
   $vector[]=rand(0,12);
}

$total=0;

for ($i=1; $i < 50; $i++) {
    echo "<p> $vector[$i] </p>";
}

for ($var=0; $var < count($vector); $var++) {
    if ($vector[$var]==7) {
        $total ++;
        echo "posición: $i";
    };
}

echo "<br>El total de 7 es:" .$total;


?>